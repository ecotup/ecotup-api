{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-len": "off",
    "object-curly-spacing": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "quotes": "off"
  }
}
